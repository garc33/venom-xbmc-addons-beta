<window>
	<defaultcontrol>1000</defaultcontrol>
	<include>dialogeffect</include>
	<coordinates>
		<system>1</system>
		<posx>0</posx>
		<posy>0</posy>
	</coordinates>
	<controls>
	
		<control type="image">
			<posx>0</posx>
			<posy>0</posy>
			<width>1280</width>
			<height>720</height>
			<texture>black-back.png</texture>
		</control>

		<control type="group">
			<animation effect="slide" start="1100,0" end="0,0" time="400" tween="quadratic" easing="out">WindowOpen</animation>
			<animation effect="slide" start="0,0" end="1100,0" time="400" tween="quadratic" easing="out">WindowClose</animation>
			<control type="image">
				<posx>310r</posx>
				<posy>0</posy>
				<width>310</width>
				<height>720</height>
				<texture border="10,0,0,0" flipx="true">MediaBladeSub.png</texture>
					<include condition="Skin.HasSetting(transphome.enabled)">transparency</include>
			</control>
			
			<control type="button">
			<description>Close Window button</description>
			<posx>290r</posx>
			<posy>0</posy>
			<width>64</width>
			<height>32</height>
			<label>-</label>
			<font>-</font>
			<onclick>PreviousMenu</onclick>
			<texturefocus>DialogCloseButton-focus2.png</texturefocus>
			<texturenofocus>DialogCloseButton.png</texturenofocus>
			<onleft>2</onleft>
			<onright>2</onright>
			<onup>2</onup>
			<ondown>2</ondown>
			<visible>system.getbool(input.enablemouse)</visible>
			</control>
		
		<control type="grouplist" id="996">
			<description>grouplist</description>
			<posx>300r</posx>
			<posy>60</posy>
			<width>300</width>
		

		<control type="button" id="1000">
			<description>button template</description>
			<include>ButtonCommonRight</include>
		</control>
	</control>
	</control>
	
	</controls>
</window>